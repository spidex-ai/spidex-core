  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:zealy": "jest --config=jest.config.zealy.js",
    "test:zealy:watch": "jest --config=jest.config.zealy.js --watch",
    "test:zealy:coverage": "jest --config=jest.config.zealy.js --coverage",
    "module:generate": "nest g resource",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migration:generate": "npm run typeorm migration:generate -d src/ormconfig.ts",
    "db:migration:create": "cd src/database/migrations && npx typeorm-ts-node-commonjs migration:create",
    "db:migration:run": "yarn typeorm migration:run -d src/ormconfig.ts",
    "db:migration:show": "yarn typeorm migration:show -d src/ormconfig.ts",
    "db:migration:revert": "yarn typeorm migration:revert -d src/ormconfig.ts",
    "db:sync": "yarn typeorm schema:sync -d src/ormconfig.ts",
    "db:drop": "yarn typeorm schema:drop -d src/ormconfig.ts",
    "db:seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "db:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ormconfig.ts",
    "db:seed:zealy": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ormconfig.ts -n ZealyQuestSeeder",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node dist/console.js",
    "core:consumer": "ts-node -r tsconfig-paths/register src/core-consumer.ts",
    "local": "ts-node -r tsconfig-paths/register src/local.ts",
    "upload-secrets": "node scripts/upload-env-to-github-secrets.js",
    "upload-secrets:dry-run": "node scripts/upload-env-to-github-secrets.js --dry-run",
    "upload-secrets:docker": "node scripts/upload-env-to-github-secrets.js --env-file=.env.docker",
    "upload-secrets:docker:dry-run": "node scripts/upload-env-to-github-secrets.js --env-file=.env.docker --dry-run",
    "script:create-users": "ts-node -r tsconfig-paths/register scripts/create-users.ts",
    "script:upload-files": "ts-node -r tsconfig-paths/register scripts/upload-files.ts"
  },
