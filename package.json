  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@constants/(.*)$": "<rootDir>/constants/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@database/(.*)$": "<rootDir>/database/$1",
      "^external/(.*)$": "<rootDir>/external/$1"
    }
